<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="432.0" prefWidth="618.0" spacing="20.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.mavenproject1.CadNota">
     <children>
        <Label alignment="TOP_CENTER" text="Cadastrar Nota" />
      <TableView fx:id="ProvasAlunos" prefHeight="200.0" prefWidth="200.0">
        <columns>
            <TableColumn fx:id="ProvaID" prefWidth="75.0" text="ID" />
          <TableColumn fx:id="ListaProvas" prefWidth="75.0" text="Provas" />
        </columns>
      </TableView>
      <TextArea fx:id="ListaAlunos" editable="false" onMouseClicked="#ClicouAluno" prefHeight="93.0" prefWidth="511.0" wrapText="true" />
      <Label alignment="TOP_CENTER" layoutX="246.0" layoutY="30.0" text="Aluno selecionado:" />
      <TextField fx:id="ALNSelecionado" layoutX="30.0" layoutY="289.0" prefHeight="25.0" prefWidth="276.0" />
      <Label alignment="TOP_LEFT" layoutX="235.0" layoutY="207.0" text="Nota:" />
      <TextField fx:id="Nota" prefHeight="13.0" prefWidth="504.0" />
      <Label fx:id="Resultado" alignment="TOP_CENTER" layoutX="279.0" layoutY="30.0" />
      <Button fx:id="Cadastrar" mnemonicParsing="false" onAction="#ColetaNota" prefHeight="37.0" prefWidth="88.0" text="Cadastrar" textOverrun="WORD_ELLIPSIS" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
